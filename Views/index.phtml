<!DOCTYPE html>
<html >
    <head >
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

        <title >Отзывы</title>

        <script type="text/javascript" src="https://code.jquery.com/jquery-3.1.0.min.js" integrity="sha256-cCueBR6CsyA4/9szpPfrX3s49M9vUU5BgtiJj06wt/s=" crossorigin="anonymous"></script>
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">

        <style type="text/css">
            body {
                margin: 50px 20px;
            }

            textarea {
                resize: none;
            }

            .sortComponent {
                margin: 0;
                padding: 10px 0;
                list-style: none;
            }

            .sortComponent__item {
                display: inline-block;
            }
            
            .sortComponent__item--active {
                font-weight: bold;
            }

            .sortComponent__item + .sortComponent__item {
                margin-left: 10px;
            }

            .thumbnail {
                max-width: 320px;
            }

            .loadingIndicator {
                top: 0;
                left: 0;
                right: 0;
                bottom: 0;
                z-index: 100;
                margin: auto;
                width: 128px;
                height: 128px;
                position: fixed;
            }
        </style>

        <script type="text/javascript">
            (function () {
                function onLoad() {
                    var __o_application = io.github.dimous.reviews.Application.getInstance();
                    ///
                    __o_application.add(
                        new io.github.dimous.reviews.models.ReviewsModel(),
                        new io.github.dimous.reviews.views.SortComponent($("#sort-component")),
                        new io.github.dimous.reviews.views.FormComponent($("#form-component")),
                        new io.github.dimous.reviews.views.WarningComponent($("#warning-component")),
                        new io.github.dimous.reviews.views.SubmitterComponent($("#submitter-component")),
                        new io.github.dimous.reviews.views.ReviewListComponent($("#review-list-component")),
                        new io.github.dimous.reviews.views.LoadingIndicatorComponent($("#loading-indicator-component"))                
                    );
                    ///
                    __o_application.run();
                }
                ///
                $(onLoad);
            }());
        </script>

        <script type="text/javascript" src="static/main.js"></script>
    </head>
    
    <body >
        <img id="loading-indicator-component" src="data:image/gif;base64," class="loadingIndicator hidden" />

        <div class="container">
            <div class="row">
                <div class="col-md-offset-8 col-md-4">
                    <ul class="sortComponent pull-right" id="sort-component">
                        <li class="sortComponent__item">Сортировать по:</li>
                        <li class="sortComponent__item">
                            <a href="#" data-sort="date" id="sort-component-by-date">По дате</a>
                        </li>
                        <li class="sortComponent__item">
                            <a href="#" data-sort="name" id="sort-component-by-name">По имени</a>
                        </li>
                        <li class="sortComponent__item">
                            <a href="#" data-sort="email" id="sort-component-by-email">По e-mail</a>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="row" id="review-list-component"></div>

            <div class="row">
                <div class="col-md-12"><hr></div>
            </div>
            
            <div class="row">
                <div class="col-md-12">
                    <ul class="text-danger hidden" id="warning-component"></ul>                    
                </div>
            </div>

            <div class="row">
                <form id="form-component" class="col-md-12" target="submitter-component" action="/review" enctype="multipart/form-data" method="post">
                    <div class="form-group">
                        <label for="form-component-name">Имя</label>
                        <input type="text" class="form-control" name="name" id="form-component-name">
                    </div>
                    <div class="form-group">
                        <label for="form-component-email">E-mail</label>
                        <input type="email" class="form-control" name="email" id="form-component-email">
                    </div>
                    <div class="form-group">
                        <label for="form-component-image">Изображение</label>
                        <input type="file" accept="image/*" name="image" id="form-component-image">
                    </div>
                    <div class="form-group">
                        <label for="form-component-text">Отзыв</label>
                        <textarea class="form-control" rows="5" name="text" id="form-component-text"></textarea>
                    </div>
                    <button type="submit" class="btn btn-primary" id="form-component-submit">Отправить</button> <button class="btn btn-default" id="form-component-preview">Просмотреть</button>
                </form>
                <iframe id="submitter-component" name="submitter-component" class="hidden"></iframe>
            </div>
        </div>
    </body>
</html>